{
  "name": "express",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=production node ./dist/bin/www",
    "dev": "ts-node-dev --poll --interval 1000 --respawn ./src/bin/www",
    "postinstall": "typesync && yarn install --ignore-scripts",
    "build": "tsc",
    "eslint": "eslint",
    "prettier": "prettier",
    "lint-fix": "yarn eslint --fix ./src/**/*.ts && yarn prettier --write ./src/**/*.ts",
    "test": "jest --forceExit --verbose"
  },
  "dependencies": {
    "@slack/webhook": "^6.0.0",
    "bootstrap": "^5.1.3",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "ejs": "^3.1.6",
    "express": "~4.16.1",
    "http-errors": "^1.8.1",
    "morgan": "~1.9.1",
    "mysql2": "^2.3.3",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.41"
  },
  "devDependencies": {
    "@types/bootstrap": "^5.1.6",
    "@types/cookie-parser": "~1.4.2",
    "@types/debug": "~4.1.7",
    "@types/ejs": "^3.1.0",
    "@types/eslint": "^8.2.1",
    "@types/express": "~4.16.1",
    "@types/http-errors": "^1.8.1",
    "@types/jest": "^27.0.3",
    "@types/morgan": "~1.9.3",
    "@types/node": "^16.11.12",
    "@types/prettier": "^2.4.2",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "eslint": "^8.4.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-prefer-arrow-functions": "^3.1.4",
    "jest": "^27.4.4",
    "prettier": "^2.5.1",
    "supertest": "^6.1.6",
    "ts-jest": "^27.1.1",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.3",
    "typesync": "^0.8.0"
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.json"
      }
    },
    "rootDir": "./src",
    "testMatch": [
      "**/__tests__/**/*.test.ts"
    ]
  }
}
